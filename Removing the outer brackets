def remove_outer_parentheses(s):
    result = []
    balance = 0 
    for i in s:
        if (i == '('):
            if (balance > 0 ):
                result.append(i)
            balance += 1 
        elif(i == ')'):
            balance -= 1 
            if (balance > 0):
                result.append(i)
    return (''.join(result))
        

# Example usage
s = "(()())(())"
print(remove_outer_parentheses(s))  # Output:Â "()()()"
