class Animal:
    def sound(self):
        print("Some generic animal sound")

class Dog(Animal):
    def sound(self):
        print("Bark")

class Cat(Animal):
    def sound(self):
        print("Meow")

# Polymorphism via method overriding
animals = [Dog(), Cat()]
for animal in animals:
    animal.sound()  # Calls the overridden method in each subclass
