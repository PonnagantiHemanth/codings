a = input().split()
c = int(input())
b = ''.join(a)
c = c%len(b)
finl = (b[c:] + b[:c])
lis = []
for i in a:
    ll = len(i)
    word = ''
    co = 0 
    for j in range (ll):
        word += finl[j]
        co += 1 
    lis.append(word)
    finl = finl[co:]
print(lis)

