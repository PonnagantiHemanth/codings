def finding_length(a):
    max_len = 0 
    for i in a:
        if (max_len < len(i)):
            max_len = len(i)
    return max_len


def perpendic(ob1,a):
    for i in range (1,ob1+1):
        per = ''
        for j in a:
            if (i <= len(j)):
                per += j[i-1]
            else:
                per += ' '
        print(per)




def main():
    a = input().split()
    ob1 = finding_length(a)
    ob2 = perpendic(ob1,a)
main()



