num = [
        1000, 900, 500, 400, 100, 90,
        50, 40, 10, 9, 5, 4, 1
    ]
val = ['M', 'CM', 'D', 'CD', 'C', 'XC', 'L', 'XL', 'X', 'IX', 'V' ,'IV', 'I'
    ]
nums = 100
res = ''
for i in range (len(num)):
    while(nums >= num[i]):
        res += val[i] 
        nums = nums - num[i]
print(res)
